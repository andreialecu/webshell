{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@formidable-webview/webshell": ["./src/index"]
    },
    "allowJs": false,
    "composite": false,
    "outDir": "./lib",
    "noEmit": false,
    "declaration": true,
    "noImplicitAny": false,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "lib", "types"]
}
