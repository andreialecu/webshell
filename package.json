{
  "name": "webshell-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "yarn workspace example start",
    "start:website": "yarn workspace website start",
    "build:website": "yarn workspace website build",
    "build:core": "yarn workspace @formidable-webview/webshell build",
    "test:acceptance": "yarn workspace acceptance-tests test",
    "test:core": "yarn workspace @formidable-webview/webshell test",
    "test:core:jest": "yarn workspace @formidable-webview/webshell test:jest",
    "test:core:ts": "yarn workspace @formidable-webview/webshell test:ts",
    "test:core:lint": "yarn workspace @formidable-webview/webshell test:lint",
    "release:core": "yarn workspace @formidable-webview/webshell release-it",
    "publish:example": "yarn workspace example expo publish",
    "publish:website": "GIT_USER=$USER USE_SSH=true yarn workspace website deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/formidable-webview/webshell.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@formidable-webview/eslint-config-webjs": "^1.0.0",
    "@react-native-community/bob": "^0.16.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-tsdoc": "^0.2.7",
    "husky": "^4.3.0",
    "prettier": "^2.1.2"
  },
  "resolutions": {
    "react": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-test-renderer": "16.13.1",
    "react-native-webview": "10.7.0"
  },
  "author": "Jules Sam. Randolph <jules.sam.randolph@gmail.com> (https://github.com/jsamr)",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
